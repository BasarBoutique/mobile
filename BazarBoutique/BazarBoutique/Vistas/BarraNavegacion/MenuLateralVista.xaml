<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BazarBoutique.Vistas.BarraNavegacion.MenuLateralVista"
             xmlns:pages="clr-namespace:BazarBoutique.Vistas.BarraNavegacion"
             NavigationPage.HasNavigationBar="False"
             xmlns:pgPrincipal="clr-namespace:BazarBoutique.Vistas.CatalogoCursosVistas">
  <FlyoutPage.Flyout>
        <pages:MenuLateralVistaFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
    
  <FlyoutPage.Detail>
    <NavigationPage >
      <x:Arguments>
        <pgPrincipal:CatalogoCursosVista/>
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>